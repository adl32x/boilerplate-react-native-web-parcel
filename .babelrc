{
  "presets": ["babel-preset-expo"],
  "env": {
    "development": {
      "plugins": ["transform-react-jsx-source"]
    },
    "web": {
      "plugins": [
        ["module-resolver", {
          "alias": {
            "^react-native$": "react-native-web"
          }
        }]
      ]
    }
  }
}
